#include "main.h"

void HAL_msp_init(void)
{
HAL_NVIC_SetPriorityGrouping(NVIC_PRIORITYGROUP_4);
SCB->SHCSR |= (0x7)<<16;
HAL_NVIC_SetPriority(MemoryManagement_IRQn,0,0);
HAL_NVIC_SetPriority(BusFault_IRQn,0,0);
HAL_NVIC_SetPriority(UsageFault_IRQn,0,0);
}

